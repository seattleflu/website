<div class="main-container">
      <header class="<%= header %> navbar navbar-expand-lg navbar-dark">
        <div class="container">
          <% if(header === 'light') {%>
            <div class="logo"><div class="first-logo dark"><a href="/"><img src="<%= siteData[0].fields.logo.fields.file.url %>" alt="<%= siteData[0].fields.logo.fields.description %>"></a></div><a href="https://brotmanbaty.org/" target="_blank" rel="noopener"><img src="/images/logo_BrotmanBatyInstitute.svg" alt="Brotman Baty Institute"></a></div>
          <%}%>
          <% if(header === 'dark') {%>
            <div class="logo"><div class="first-logo"><a href="/"><img src="<%= siteData[0].fields.logoWhite.fields.file.url %>" alt="<%= siteData[0].fields.logoWhite.fields.description %>"></a></div><a href="https://brotmanbaty.org/" target="_blank" rel="noopener"><img src="/images/Logo_BrotmanBaty_Reverse.svg" alt="Brotman Baty Institute"></a></div>
          <%}%>
          <% if(nav == 'true'){ %>
            <div class="right-menu">
              <span class="toggle-button <% if(header === 'light') {%>light<%}%>" id="toggle-button">
                  <div class="menu-bar menu-bar-top"></div>
                  <div class="menu-bar menu-bar-middle"></div>
                  <div class="menu-bar menu-bar-bottom"></div>
              </span>
              <div class="menu-wrap" id="menu-wrap">
                  <div class="menu-sidebar">
                      
                      <div class="menu-main-menu-container">
                          <ul id="menu-main-menu" class="menu">
                            <% siteData[0].fields.menuMain.forEach((item) => { %>
                            <li id="<%= item.fields.name %>" class="menu-item"><a href="<%= "/" + item.fields.urlSegment %>"><%= item.fields.name %></a></li>
                            <% }) %>
                            </ul>
                            <% if(enroll == 'true'){ %>
                              <ul style="margin:0; padding: 0; list-style: none;">
                              <li class="join-the-study"><a href="/enroll" class="nav-item"><button>Join the Study</button></a></li></ul>
                            <%}%>  
                      </div>         
                  </div>
              </div>
          </div>
  
          <%}%>
          <% if(nav == 'true'){ %>
          <ul class="navbar-nav ml-auto">
          <% siteData[0].fields.menuMain.forEach((item) => { %>
          <li id="<%= item.fields.name %>" class="nav-item"><a href="<%= "/" + item.fields.urlSegment %>"><%= item.fields.name %></a></li>
          <% }) %>
          </ul>    
          <%}%>
          <% if(enroll == 'true'){ %><ul class="navbar-nav" style="margin:0; padding: 0; list-style: none;
          "><li class="join-the-study"><a href="/enroll" class="nav-item"><button>Join the Study</button></a></li></ul> <%}%>
        </div>
      </header>
